pipeline {
  agent any
  stages {
    stage('download') {
      steps {
        echo 'download completed'
      }
    }

    stage('Build') {
      steps {
        echo 'build completed'
      }
    }

    stage('test') {
      steps {
        echo 'done testing'
      }
    }

    stage('deployy') {
      steps {
        echo 'we did it'
      }
    }

    stage('deliver') {
      steps {
        echo 'product at your home'
      }
    }

  }
}